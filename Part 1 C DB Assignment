SELECT IFNULL(seller_account_ref, 'N/A') AS 'Seller Account Ref', IFNULL(seller_name, 'N/A') AS 'Seller Name', pr.product_code, pr.product_description, COUNT(op.order_id) AS 'Number of Orders', IFNULL(SUM(quantity),0) AS 'Total Quantity Sold', 'Â£' || IFNULL(ROUND(SUM(op.quantity *op.price),2),0) AS 'Total Value of Sales', order_date
FROM products pr
        LEFT OUTER JOIN  ordered_products op on pr.product_id = op.product_id
        LEFT OUTER JOIN  sellers se on op.seller_id = se.seller_id
        LEFT OUTER JOIN  shopper_orders so on op.order_id = so.order_id

WHERE order_date IS NULL OR
order_date > '2019-06-01'
GROUP BY seller_name, product_description;